{
  "manifest_version": 3,
  "name": "MinWinTidHelper",
  "version": "0.7",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": ["https://*.no/Maintenance"],
  "web_accessible_resources": [{
    "resources" : ["music/elevator.mp3", "helpers.js"],
    "matches": ["<all_urls>"]
  }],
  "content_scripts": [
    {
      "matches": [
        "https://*.no/Maintenance"
      ],
      "js": ["jquery.js", "content.js"]
    }
  ],
  "icons": { "48": "owl-head.png" }
}
